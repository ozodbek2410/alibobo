{
  "name": "alibobo-react",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "chart.js": "^4.5.0",
    "mongodb": "^6.18.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10"
  },
  "scripts": {
    "start": "cross-env DISABLE_ESLINT_PLUGIN=true ESLINT_NO_DEV_ERRORS=true concurrently \"npm run client\" \"npm run server\"",
    "client": "cross-env DISABLE_ESLINT_PLUGIN=true ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "server": "cd backend && npm start",
    "build": "cross-env CI=false react-scripts build",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze-bundle",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "cross-env DISABLE_ESLINT_PLUGIN=true ESLINT_NO_DEV_ERRORS=true concurrently \"npm run client\" \"cd backend && npm run dev\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
